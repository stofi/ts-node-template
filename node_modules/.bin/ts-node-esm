#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules/ts-node/dist/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules/ts-node/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules/ts-node/dist/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules/ts-node/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/ts-node@10.9.1_mwktwrqic243a56fotetixsg4q/node_modules:/Users/kurishutofu/Code/templates/ts-node-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-esm.js" "$@"
fi
